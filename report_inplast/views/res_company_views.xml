<?xml version='1.0' encoding='UTF-8'?>
<odoo>

    <record id="pnt_inplast_res_company_form" model="ir.ui.view">
        <field name="name">inplast.report.res.company.form</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <data>
                <xpath expr="//notebook" position="inside">
                <page name="Reports">
                    <group name="reports_field">
                        <field name="pnt_goods_state_table" widget="image"/>
                        <field name="pnt_logo_cert_uno" widget="image"/>
                        <field name="pnt_logo_cert_dos" widget="image"/>
                        <field name="pnt_logo_cert_tres" widget="image"/>
                    </group>
                </page>
            </xpath>
            </data>
        </field>
        <field name="priority">99</field>
        <field name="type">form</field>
    </record>


</odoo>